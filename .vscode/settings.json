{
    "cmake.configureOnOpen": true,
    "cmake.configureArgs": ["-DTOOLS=1","-DCMAKE_INSTALL_PREFIX=/workspaces/skyfire-server"],
    "cmake.generator": "Unix Makefiles",
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.tcc": "cpp",
        "optional": "cpp",
        "memory": "cpp",
        "numeric": "cpp",
        "cmath": "cpp",
        "*.inc": "cpp",
        "*.def": "cpp"
    }
}